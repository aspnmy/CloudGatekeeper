{
    "name": "cloudgatekeeper",
    "version": "1.0.0",
    "description": "A dynamic routing proxy system based on Cloudflare Workers with rule-based request handling",
    "main": "index.js",
    "scripts": {
        "setup": "yarn install && yarn wrangler login",
        "login": "yarn wrangler login",
        "dev": "yarn wrangler --env dev --remote",
        "debug": "npx wrangler deploy --env dev ",
        "build:dev": "wrangler build --env dev",
        "deploy:dev": "wrangler deploy --env dev",
        "build:prod": "wrangler build --env production",
        "deploy:prod": "wrangler deploy --env production",
        "check:env": "node scripts/check-env.js",
        "publish": "yarn check:env && yarn build:prod && yarn deploy:prod",
        "format": "prettier --write 'src/*.{js,css,json,md}'",
        "prestart": "node scripts/check-node.js"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20230710.1",
        "wrangler": "^4.0.0"
    },
    "dependencies": {
        "cloudgatekeeper": "file:"
    },
    "engines": {
        "node": ">=20.14.0",
        "yarn": ">=1.22.0"
    },
    "engineStrict": true
}